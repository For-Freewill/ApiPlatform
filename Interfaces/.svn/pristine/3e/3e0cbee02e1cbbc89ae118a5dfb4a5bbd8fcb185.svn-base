*{padding:0; margin:0;}
a{ text-decoration:none;}
li,ul{ list-style:none;}
img{ border:none;}
input{vertical-align:middle;}
.clear{ clear:both; height:0; width:100%; overflow:hidden;}
.tai_header{ width:100%; height:68px; padding-top:10px; background:#000;}
.tai_logo{ width:430px; height:54px; margin-left:30px;}
.tai_logo dt{ float:left; margin-right:10px;}
.tai_logo dd{ width:360px; height:54px; line-height:54px; float:left; color:#FFF; font-size:20px; font-family:"微软雅黑"; font-weight:600;}
.tai_header p{ float:right; width:500px; height:54px; color:#fff; line-height:54px; text-align:right;}
.tai_header p a{ color:#fff; padding:0 6px;}
.tai_main{ width:100%; height:auto; }
.deng_c{ }
.tai_left{ width:208px; height:auto; padding-top:35px; background:#f8f8f8 url(../images/ying_03.jpg) repeat-y right 0; padding-right:5px; float:left;}
.tai_left p{ background:url(../images/hou_19.jpg) repeat-x; width:179px; height:39px; border-top:1px solid #d8d8d8;border-bottom:1px solid #d8d8d8; line-height:39px; padding-left:27px;font-size:14px; color:#475055; font-family:"微软雅黑";}
.tai_left p img{ margin-right:8px; margin-top:10px;}
.tai_left ul{ padding-left:54px; height:auto;}
.tai_left ul li{ height:40px; line-height:40px;}
.tai_left ul li a{ font-size:14px; color:#475055; font-family:"微软雅黑";}
.tai_right{ margin-left:28px;margin-right:28px; padding-top:30px;_margin-left:0; _float:left;}
.table_da{ background:#fff;}
.table_da td{ height:48px; line-height:48px; text-align:center; font-size:13px;font-family:"微软雅黑"}
.back_tit{ background:url(../images/hou_11.jpg) repeat-x; height:50px; line-height:50px;}
.back_tit td{ text-align:center; line-height:50px; color:#FFF; font-size:14px;}
.hang01{ background:#e4eaf2;}
.hang02{ background:#d3dfee;}
.gaic{ padding:6px 10px; background:#fff; border:1px solid #dfdfdf; font-size:14px; color:#676f74; font-family:"微软雅黑";}
.yeshu{ height:30px; line-height:30px; color:#475055; text-align:center;font-size:13px;}
.yeshu a{color:#475055; padding:0 7px; text-decoration:underline; }
/*登陆*/
.deng_main{ width:1002px; height:auto;  padding-top:21px;_padding-top:20px; margin:0 auto; background:url(../images/bg.png) no-repeat right 0;}
.deng_c{ height:500px;}
.deng_right{ width:377px; height:500px; float:left;}
.deng_right form{ width:447px; height:238px; margin-top:24px;_margin-top:20px; background:url(../images/line.jpg) no-repeat; padding:34px 35px 0 70px;} 
.kuan_l{ width:295px;  margin-right:15px;}
.yhm{ width:267px; height:26px; line-height:26px; border:1px solid #a5b7ca; border-radius:5px; margin-bottom:14px; padding-left:28px; color:#666; background:#fff url(../images/deng_08.jpg) no-repeat 6px center;}
.wjmm{ height:25px;}
.wjmm a{ font-size:12px; color:#2c2c2c; text-decoration:underline;}
.wjmm  span{font-size:12px; color:#2c2c2c;}
.pswd{background:#fff url(../images/deng_12.jpg) no-repeat 6px center; }
.dengl{ background:url(../images/dd_07.png) no-repeat; width:100px; height:34px;  border:none; clear:both; margin-right:32px;}
.zhuc{ background:url(../images/dd_09.png) no-repeat; width:100px; height:34px; border:none;  clear:both;}
.yzhengm{ width:98px; height:26px; border:1px solid #a5b7ca; border-radius:5px; margin-right:7px; margin-top:0; margin-bottom:10px; float:left;}
#footer {height:195px;background:#1d1d1d; font-size:12px; font-family:"微软雅黑";}
/*页面底部版权-基础*/
